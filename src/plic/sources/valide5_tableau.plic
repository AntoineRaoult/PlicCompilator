programme essai {
    entier i ;
    entier j ;
    tableau [ 5 ] tab ;
    tableau [ 5 ] tabb ;
    i := 1 ;
    j := 2 ;
    tab [ i + j ] := 1 ;
    ecrire tab [ i + j ] ;

    tabb [ tab [ i + j ] ] := 2 ;
    ecrire tabb [ tab [ i + j ] ] ;

    tab [ i + j ] := tabb [ tab [ i + j ] ] ;
    ecrire tab [ i + j ] ;
}