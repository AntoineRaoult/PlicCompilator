// un exemple
programme test {
entier k ;
entier i ;
tableau [ 5 ] tab ; // un tableau de 8 entiers
k := 3 ;
i := 2 ;
tab [ k ] := k ;
tab [ i ] := 5 ;
ecrire tab [ k ] ; // 3
ecrire tab [ i ] ; // 5
i := tab [ k ] ; // i = 3
ecrire i ; // 3
ecrire tab [ k ] ;
tab [ k ] := tab [ 2 ] ; // tab[k] = 5
ecrire tab [ k ] ; // 5

// sortie attendue : 3 5 3 3 5
}